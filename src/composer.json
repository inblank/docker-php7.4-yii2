{
    "name": "k2/microservice-template",
    "license": "proprietary",
    "description": "Microservice base structure template",
    "keywords": [
        "microservice", "template", "kartoteka"
    ],
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "~7.4",
        "ext-json": "*",
        "yiisoft/yii2": "~2.0.0",
        "yiisoft/yii2-swiftmailer": "~2.1.0",
        "inblank/yii2-console": "dev-main",
        "cranetm/yii2-json-rpc-2.0": "~1.2.0",
        "guzzlehttp/guzzle": "~7.3",
        "league/flysystem": "~2.2.0"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "~2.0",
        "yiisoft/yii2-gii": "~2.0",
        "yiisoft/yii2-faker": "~2.0",
        "phpunit/phpunit": "~9.5.0",
        "roave/security-advisories": "dev-latest"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ],
    "config": {
        "secure-http": false,
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
